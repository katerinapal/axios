{
    "name": "axios",
    "version": "0.19.0-beta.1",
    "description": "Promise based HTTP client for the browser and node.js",
    "main": "index.js",
    "scripts": {
        "test": "grunt test && bundlesize",
        "start": "node ./sandbox/server.js",
        "build": "NODE_ENV=production grunt build",
        "preversion": "npm test",
        "version": "npm run build && grunt version && git add -A dist && git add CHANGELOG.md bower.json package.json",
        "postversion": "git push && git push --tags",
        "examples": "node ./examples/server.js",
        "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "fix": "eslint --fix lib/**/*.js",
        "compile-eslintrcjs": "babel .eslintrc.js -o .eslintrc.js --require babel-polyfill --presets es2015-without-strict",
        "compile-distaxiosjs": "babel dist\\axios.js -o dist\\axios.js --require babel-polyfill --presets env",
        "compile-distaxiosminjs": "babel dist\\axios.min.js -o dist\\axios.min.js --require babel-polyfill --presets env",
        "compile-examplesgetserverjs": "babel examples\\get\\server.js -o examples\\get\\server.js --require babel-polyfill --presets es2015-without-strict",
        "compile-examplespostserverjs": "babel examples\\post\\server.js -o examples\\post\\server.js --require babel-polyfill --presets es2015-without-strict",
        "compile-examplesserverjs": "babel examples\\server.js -o examples\\server.js --require babel-polyfill --presets es2015-without-strict",
        "compile-examplesuploadserverjs": "babel examples\\upload\\server.js -o examples\\upload\\server.js --require babel-polyfill --presets es2015-without-strict",
        "compile-Gruntfilejs": "babel Gruntfile.js -o Gruntfile.js --require babel-polyfill --presets es2015-without-strict",
        "compile-indexjs": "babel index.js -o index.js --require babel-polyfill --presets es2015-without-strict",
        "compile-karmaconfjs": "babel karma.conf.js -o karma.conf.js --require babel-polyfill --presets es2015-without-strict",
        "compile-libadaptershttpjs": "babel lib\\adapters\\http.js -o lib\\adapters\\http.js --require babel-polyfill --presets env",
        "compile-libadaptersxhrjs": "babel lib\\adapters\\xhr.js -o lib\\adapters\\xhr.js --require babel-polyfill --presets env",
        "compile-libaxiosjs": "babel lib\\axios.js -o lib\\axios.js --require babel-polyfill --presets env",
        "compile-libcancelCanceljs": "babel lib\\cancel\\Cancel.js -o lib\\cancel\\Cancel.js --require babel-polyfill --presets env",
        "compile-libcancelCancelTokenjs": "babel lib\\cancel\\CancelToken.js -o lib\\cancel\\CancelToken.js --require babel-polyfill --presets env",
        "compile-libcancelisCanceljs": "babel lib\\cancel\\isCancel.js -o lib\\cancel\\isCancel.js --require babel-polyfill --presets env",
        "compile-libcoreAxiosjs": "babel lib\\core\\Axios.js -o lib\\core\\Axios.js --require babel-polyfill --presets env",
        "compile-libcorecreateErrorjs": "babel lib\\core\\createError.js -o lib\\core\\createError.js --require babel-polyfill --presets env",
        "compile-libcoredispatchRequestjs": "babel lib\\core\\dispatchRequest.js -o lib\\core\\dispatchRequest.js --require babel-polyfill --presets env",
        "compile-libcoreenhanceErrorjs": "babel lib\\core\\enhanceError.js -o lib\\core\\enhanceError.js --require babel-polyfill --presets env",
        "compile-libcoreInterceptorManagerjs": "babel lib\\core\\InterceptorManager.js -o lib\\core\\InterceptorManager.js --require babel-polyfill --presets env",
        "compile-libcoremergeConfigjs": "babel lib\\core\\mergeConfig.js -o lib\\core\\mergeConfig.js --require babel-polyfill --presets env",
        "compile-libcoresettlejs": "babel lib\\core\\settle.js -o lib\\core\\settle.js --require babel-polyfill --presets env",
        "compile-libcoretransformDatajs": "babel lib\\core\\transformData.js -o lib\\core\\transformData.js --require babel-polyfill --presets env",
        "compile-libdefaultsjs": "babel lib\\defaults.js -o lib\\defaults.js --require babel-polyfill --presets env",
        "compile-libhelpersbindjs": "babel lib\\helpers\\bind.js -o lib\\helpers\\bind.js --require babel-polyfill --presets env",
        "compile-libhelpersbuildURLjs": "babel lib\\helpers\\buildURL.js -o lib\\helpers\\buildURL.js --require babel-polyfill --presets env",
        "compile-libhelperscombineURLsjs": "babel lib\\helpers\\combineURLs.js -o lib\\helpers\\combineURLs.js --require babel-polyfill --presets env",
        "compile-libhelperscookiesjs": "babel lib\\helpers\\cookies.js -o lib\\helpers\\cookies.js --require babel-polyfill --presets env",
        "compile-libhelpersdeprecatedMethodjs": "babel lib\\helpers\\deprecatedMethod.js -o lib\\helpers\\deprecatedMethod.js --require babel-polyfill --presets env",
        "compile-libhelpersisAbsoluteURLjs": "babel lib\\helpers\\isAbsoluteURL.js -o lib\\helpers\\isAbsoluteURL.js --require babel-polyfill --presets env",
        "compile-libhelpersisURLSameOriginjs": "babel lib\\helpers\\isURLSameOrigin.js -o lib\\helpers\\isURLSameOrigin.js --require babel-polyfill --presets env",
        "compile-libhelpersnormalizeHeaderNamejs": "babel lib\\helpers\\normalizeHeaderName.js -o lib\\helpers\\normalizeHeaderName.js --require babel-polyfill --presets env",
        "compile-libhelpersparseHeadersjs": "babel lib\\helpers\\parseHeaders.js -o lib\\helpers\\parseHeaders.js --require babel-polyfill --presets env",
        "compile-libhelpersspreadjs": "babel lib\\helpers\\spread.js -o lib\\helpers\\spread.js --require babel-polyfill --presets env",
        "compile-libutilsjs": "babel lib\\utils.js -o lib\\utils.js --require babel-polyfill --presets env",
        "compile-sandboxclientjs": "babel sandbox\\client.js -o sandbox\\client.js --require babel-polyfill --presets es2015-without-strict",
        "compile-sandboxserverjs": "babel sandbox\\server.js -o sandbox\\server.js --require babel-polyfill --presets es2015-without-strict",
        "compile-webpackconfigjs": "babel webpack.config.js -o webpack.config.js --require babel-polyfill --presets es2015-without-strict",
        "compile-1_script": "npm run compile-eslintrcjs && npm run compile-distaxiosjs && npm run compile-distaxiosminjs && npm run compile-examplesgetserverjs && npm run compile-examplespostserverjs && npm run compile-examplesserverjs && npm run compile-examplesuploadserverjs && npm run compile-Gruntfilejs && npm run compile-indexjs && npm run compile-karmaconfjs && npm run compile-libadaptershttpjs && npm run compile-libadaptersxhrjs && npm run compile-libaxiosjs && npm run compile-libcancelCanceljs && npm run compile-libcancelCancelTokenjs && npm run compile-libcancelisCanceljs && npm run compile-libcoreAxiosjs && npm run compile-libcorecreateErrorjs && npm run compile-libcoredispatchRequestjs && npm run compile-libcoreenhanceErrorjs",
        "compile-2_script": "npm run compile-libcoreenhanceErrorjs && npm run compile-libcoreInterceptorManagerjs && npm run compile-libcoremergeConfigjs && npm run compile-libcoresettlejs && npm run compile-libcoretransformDatajs && npm run compile-libdefaultsjs && npm run compile-libhelpersbindjs && npm run compile-libhelpersbuildURLjs && npm run compile-libhelperscombineURLsjs && npm run compile-libhelperscookiesjs && npm run compile-libhelpersdeprecatedMethodjs && npm run compile-libhelpersisAbsoluteURLjs && npm run compile-libhelpersisURLSameOriginjs && npm run compile-libhelpersnormalizeHeaderNamejs && npm run compile-libhelpersparseHeadersjs && npm run compile-libhelpersspreadjs && npm run compile-libutilsjs && npm run compile-sandboxclientjs && npm run compile-sandboxserverjs && npm run compile-webpackconfigjs",
        "compile": "npm run compile-1_script && npm run compile-2_script"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/axios/axios.git"
    },
    "keywords": [
        "xhr",
        "http",
        "ajax",
        "promise",
        "node"
    ],
    "author": "Matt Zabriskie",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/axios/axios/issues"
    },
    "homepage": "https://github.com/axios/axios",
    "devDependencies": {
        "bundlesize": "^0.17.0",
        "coveralls": "^3.0.0",
        "es6-promise": "^4.2.4",
        "grunt": "^1.0.2",
        "grunt-banner": "^0.6.0",
        "grunt-cli": "^1.2.0",
        "grunt-contrib-clean": "^1.1.0",
        "grunt-contrib-watch": "^1.0.0",
        "grunt-eslint": "^20.1.0",
        "grunt-karma": "^2.0.0",
        "grunt-mocha-test": "^0.13.3",
        "grunt-ts": "^6.0.0-beta.19",
        "grunt-webpack": "^1.0.18",
        "istanbul-instrumenter-loader": "^1.0.0",
        "jasmine-core": "^2.4.1",
        "karma": "^1.3.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.1.1",
        "karma-firefox-launcher": "^1.1.0",
        "karma-jasmine": "^1.1.1",
        "karma-jasmine-ajax": "^0.1.13",
        "karma-opera-launcher": "^1.0.0",
        "karma-safari-launcher": "^1.0.0",
        "karma-sauce-launcher": "^1.2.0",
        "karma-sinon": "^1.0.5",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^1.7.0",
        "load-grunt-tasks": "^3.5.2",
        "minimist": "^1.2.0",
        "mocha": "^5.2.0",
        "sinon": "^4.5.0",
        "typescript": "^2.8.1",
        "url-search-params": "^0.10.0",
        "webpack": "^1.13.1",
        "webpack-dev-server": "^1.14.1",
        "babel-cli": "latest",
        "babel-core": "latest",
        "babel-plugin-add-module-exports": "latest",
        "babel-plugin-transform-es2015-modules-commonjs": "latest",
        "babel-polyfill": "latest",
        "babel-preset-env": "latest",
        "babel-preset-es2015-without-strict": "latest"
    },
    "browser": {
        "./lib/adapters/http.js": "./lib/adapters/xhr.js"
    },
    "typings": "./index.d.ts",
    "dependencies": {
        "follow-redirects": "1.5.10",
        "is-buffer": "^2.0.2"
    },
    "bundlesize": [
        {
            "path": "./dist/axios.min.js",
            "threshold": "5kB"
        }
    ]
}
